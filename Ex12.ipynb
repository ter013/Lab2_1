    "import numpy as np\n",
    "import turtle\n",
    "\n",
    "turtle.shape('turtle')\n",
    "turtle.speed(0)\n",
    "\n",
    "turtle.left(90)\n",
    "\n",
    "def Dug(R):\n",
    "    for i in range(0,180,1):\n",
    "        turtle.forward(2*R*np.sin(1/360*np.pi))\n",
    "        turtle.right(1)\n",
    "        \n",
    "for t in range(0,10,1):\n",
    "    Dug(60)\n",
    "    Dug(20)"
   ]
